{
  "indentation": {
    "type": "spaces",
    "size": 2
  },
  "quotes": {
    "type": "single",
    "exceptions": [
      "jsx-double"
    ]
  },
  "semicolons": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "endOfLine": "lf",
  "lineWidth": 100
}
